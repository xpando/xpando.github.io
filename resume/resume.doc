      <w:r wsp:rsidR="00C146CA" wsp:rsidRPr="00C146CA">
        <w:t>Sr. Software Engineer, </w:t>
      </w:r>
      <w:hlink w:dest="https://amazon.com">
        <w:r wsp:rsidR="009452CA" wsp:rsidRPr="00606071">
          <w:rPr>
            <w:rStyle w:val="Hyperlink"/>
