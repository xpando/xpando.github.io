          <wx:font wx:val="Calibri"/>
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20"/>
        </w:rPr>
      </w:pPr>
      <w:r><w:rPr></w:rPr><w:t>Prototyped realtime click/open rate dashboard using ElasticSearch and Kibana for a team hackathon.</w:t></w:r>
    </w:p>

